page{
  background: #f8f8f8;
}
.add_address{
  margin-left: 24rpx;
  margin-top: 38rpx;
  border-radius: 50rpx;
  background: #fff;
  width: 702rpx;
  padding-bottom: 48rpx;
}
.add_address_list{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding: 0 10rpx 0 24rpx;
  /* border: 1px solid black; */
}
.add_address_list_item{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding: 38rpx 10rpx 12rpx 30rpx;
  /* border: 1px solid red; */
}


.add_address_top_common.is_checked {
  border: 2rpx solid #f8809a;
  background: #fff;
  color: #f8809a
}






/* 弹出层 */
.mask{
 width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 990;
    opacity: 0.2;

}
.tips_window{
 height: 500rpx;
 width: 100%;
 overflow: hidden;
 position: fixed;
 bottom: 0;
 left: 0;
 z-index: 2000;
 background: #fff;
 padding-top: 20rpx;
}
.tips_window_top{
   display: flex;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  line-break: 90rpx;
  background: #fff;
  padding: 0 46rpx;
  /* border: 1px solid red; */
  font-size:34rpx;
}
.select{
  width: 99%;
  height: 288rpx;
  /* border: 1px solid red; */
  margin-top: 50rpx;
}

.picker-item {
 line-height: 70rpx;
 margin-left: 5rpx;
 margin-right: 5rpx;
 font-size: 30rpx;
 text-align: center;
}
.add_address_list_item_left{
  font-size: 28rpx;
  color: #464646;
}
.add_address_list_item_input{
  padding-left: 10rpx;
  border-bottom: 2rpx solid #eeeced;
  font-size: 28rpx;
  color: #464646;
  width: 500rpx;
}

.add_margin_left{
  margin-left: 30rpx;
}

.add_address_list_item_right{
  color: #b9b9b9;
  font-size: 28rpx;
}

.add_address_top{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding-bottom: 18rpx;
  padding-top: 40rpx;
  border-bottom: 2rpx solid #eeeced;
  padding-left: 120rpx;
  margin-left: 16rpx;
}

.add_address_top_common{
  font-size: 25rpx;
  color: #929292;
  text-align: center;
  width:74rpx;
  height: 35rpx;
  line-height: 35rpx; 
  border: 2rpx solid #929292;
  border-radius: 10rpx;
  }

  .address_woman{
    margin-left: 28rpx;
  }
  .add_address_list_right{
    display: flex;
    justify-content: flex-start;
    padding-left: 80rpx;

  }

